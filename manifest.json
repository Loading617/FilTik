{
  "manifest_version": 3,
  "name": "FilTik",
  "version": "1.0",
  "default_locale": "en",
  "description": "Filter TikTok videos on Reposts, Favorites, and Liked pages.",
  "homepage_url": "https://filtik.com",
  
    "action": {
    "default_popup": "popup.html"
  },

      "icons": {
      "16": "icons/FilTik16.png",
      "32": "icons/FilTik32.png",
      "48": "icons/FilTik48.png",
      "128": "icons/FilTik128.png"
  },

  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },

  "permissions": [
    "storage",
    "scripting"
 ],

  "background": {
    "service_worker": "background.js"
  }
}
